		<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
		<PackageProjectUrl>https://github.com/jiniannet/JntemplateViewEngine</PackageProjectUrl>
		<PackageIcon>icon.png</PackageIcon>
		<RepositoryUrl>https://gitee.com/jiniannet/JntemplateViewEngine.git</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>jntemplate viewengine</PackageTags>
		<AssemblyName>JinianNet.AspNetCoreViewEngine.Jntemplate</AssemblyName>
		<AssemblyOriginatorKeyFile>jiniannet.snk</AssemblyOriginatorKeyFile>
	</PropertyGroup> 

