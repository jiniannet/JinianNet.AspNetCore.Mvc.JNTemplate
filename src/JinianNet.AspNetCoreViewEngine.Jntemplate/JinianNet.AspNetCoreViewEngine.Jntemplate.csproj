<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;netstandard2.1;netcoreapp2.1;netcoreapp3.1;net46;net5.0</TargetFrameworks>
		<SignAssembly>false</SignAssembly>
		<Version>0.0.2</Version>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
		<AssemblyVersion>0.0.2.0</AssemblyVersion>
		<FileVersion>0.0.2.0</FileVersion>
		<TreatWarningsAsErrors>True</TreatWarningsAsErrors>
		<TreatSpecificWarningsAsErrors />
		<NoWarn>1701;1702</NoWarn>
		<Optimize>True</Optimize>
		<PackageId>JinianNet.AspNetCoreViewEngine.Jntemplate</PackageId>
		<Authors>JinianNet.com</Authors>
		<Company>JinianNet.com</Company>
		<Product>AspNetCoreViewEngine.Jntemplate</Product>
